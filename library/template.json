          "videoFile": "{{seasons[season].episodes[episode].video_filepath}}",
          "subtitleFile": "{{seasons[season].episodes[episode].subtitle_filepath}}" },
          
  {%  endfor  %}
        ] },

  },

{%  endfor %}
]
